   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Removing outliers from Income\n",
      "Old Data Shape: (2216, 31)\n",
      "=======\n",
      "New Data Shape: (2208, 31)\n",
      "Removing outliers from age\n",
      "Old Data Shape: (2208, 31)\n",
      "=======\n",
      "New Data Shape: (2205, 31)\n"
     ]
    }
   ],
   "source": [
    "for col_name in ['Income', 'age']:\n",
    "    # Quartile 1\n",
    "    q1 = np.percentile(marketing_campaign[col_name], 25, method='midpoint')\n",
    "    \n",
    "    # Quartile 3\n",
    "    q3 = np.percentile(marketing_campaign[col_name], 75, method='midpoint')\n",
    "    \n",
    "    # IQR\n",
    "    IQR = q3 - q1\n",
    "    print(\"Removing outliers from\", col_name)\n",
    "    print(\"Old Data Shape:\", marketing_campaign.shape)\n",
    "    print(\"=======\")\n",
    "    \n",
    "    # Upper bound\n",
    "    upper = np.where(marketing_campaign[col_name] >= (q3 + 1.5 * IQR))\n",
    "    \n",
    "    # Lower bound\n",
    "    lower = np.where(marketing_campaign[col_name] <= (q1 - 1.5 * IQR))\n",
    "    \n",
    "    # Removing the outliers\n",
    "    marketing_campaign.drop(upper[0], inplace=True)\n",
    "    marketing_campaign.drop(lower[0], inplace=True)\n",
    "    \n",
    "    print(\"New Data Shape:\", marketing_campaign.shape)\n",
    "    \n",
    "    marketing_campaign.reset_index(inplace=True, drop=True)"
   ]
  },
