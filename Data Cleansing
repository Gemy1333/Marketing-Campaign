  {
   "cell_type": "code",
   "execution_count": 95,
   "id": "4f6531d4-c184-4b20-afa4-80d1602f3efd",
   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "##RFM Analysis\n",
    "r = pd.qcut(marketing_campaign['Recency'], q=5, labels=range(5, 0, -1), duplicates='drop')\n",
    "f = pd.qcut(marketing_campaign['Frequency'], q=5, labels=range(1, 6))\n",
    "m = pd.qcut(marketing_campaign['Monetary (total_spending)'], q=5, labels=range(1, 6))\n",
    "\n",
    "marketing_campaign = marketing_campaign.assign(R=r.values, F=f.values, M=m.values)\n",
    "marketing_campaign[['R', 'F', 'M']] = marketing_campaign[['R', 'F', 'M']].astype('int64')\n",
    "marketing_campaign['Avg_F_M'] = marketing_campaign[['F', 'M']].mean(axis=1)\n",
    "marketing_campaign['Avg_F_M'] = marketing_campaign['Avg_F_M'].astype('int64')\n",
    "\n",
    "# Create a function to assign labels based on R, F, and M values\n",
    "def categorize_rfm_avg(row):\n",
    "    r, avg_f_m = row['R'], row['Avg_F_M']\n",
    "    if r >= 4 and avg_f_m >= 4:\n",
    "        return 'Champions'\n",
    "    elif (r >= 4 and avg_f_m == 2) or (r >= 3 and avg_f_m == 3):\n",
    "        return 'Potential Loyalists'\n",
    "    elif (r == 5 and avg_f_m == 3) or (r == 4 and avg_f_m == 4) or (r == 3 and avg_f_m >= 4):\n",
    "        return 'Loyal Customers'\n",
    "    elif r == 5 and avg_f_m == 1:\n",
    "        return 'Recent Customers'\n",
    "    elif (r == 4 and avg_f_m == 1) or (r == 3 and avg_f_m == 1):\n",
    "        return 'Promising'\n",
    "    elif (r == 3 and avg_f_m == 2) or (r == 2 and avg_f_m == 3) or (r == 2 and avg_f_m >= 2):\n",
    "        return 'Customers Needing Attention'\n",
    "    elif (r == 2 and avg_f_m == 5) or (r == 2 and avg_f_m == 4) or (r == 1 and avg_f_m >= 3):\n",
    "        return 'At Risk'\n",
    "    elif (r == 1 and avg_f_m == 5) or (r == 1 and avg_f_m == 4):\n",
    "        return \"Can't Lose Them\"\n",
    "    elif r == 1 and avg_f_m == 2:\n",
    "        return 'Hibernating'\n",
    "    else:\n",
    "        return 'Lost'\n",
    "\n",
    "# Apply the function to create a new column 'RFM Category'\n",
    "marketing_campaign['RFM Category'] = marketing_campaign.apply(categorize_rfm_avg, axis=1)\n",
    "\n",
    "with pd.ExcelWriter('marketing_campaign_cleaned.xlsx') as writer:\n",
    "    \n",
    "     marketing_campaign.to_excel(writer, sheet_name='data', index=False)"
   ]
  },
